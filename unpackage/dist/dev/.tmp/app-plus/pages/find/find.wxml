<view><cu-custom vue-id="1" bgColor="kpbg-black" isBack="{{false}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content"><view class="text-white">开趴 - KP</view></view></cu-custom><view class="cu-item text-center padding-lg kpbg" style="{{'height:360rpx;'+('background-image:'+('url('+userData.avatar+')')+';')}}"><view class="kp-drag"><view class="cu-avatar round margin-top-xs" style="{{'height:140rpx;width:140rpx;'+('background-image:'+('url('+userData.avatar+')')+';')}}"></view><view class=" text-cut margin-auto text-lg  padding -xs" style="text-shadow:6rpx 6rpx 8rpx rgba(26, 26, 26, 0.2);">{{''+userData.nickname+''}}</view><view class="margin-auto"><view data-event-opts="{{[['tap',[['rel',['$0'],['uid']]]]]}}" class="cu-tag round bg-gray padding-lr margin-right" bindtap="__e">发布</view><view data-event-opts="{{[['tap',[['myrel',['$0'],['uid']]]]]}}" class="cu-tag round bg-yellow padding-lr margin-left" bindtap="__e">我发布的</view><view data-event-opts="{{[['tap',[['getfind']]]]}}" class="cu-tag round bg-yellow padding-lr margin-left" bindtap="__e">刷新</view></view></view></view><block wx:for="{{findListres.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-card dynamic margin-top-sm',isCard?'no-card':'no-card']}}"><view data-event-opts="{{[['tap',[['detail',['$0'],[[['findListres.list','',index,'fid']]]]]]]}}" class="cu-item shadow" bindtap="__e"><view class="cu-list menu-avatar"><view class="cu-item"><view class="cu-avatar round lg" style="{{'background-image:'+('url('+item.findUser.avatar+')')+';'}}"></view><view class="content flex-sub"><view class="text-cut" style="width:80%;height:60rpx;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;">{{item.findUser.nickname}}</view><view class="text-gray text-sm flex justify-between">{{item.createTime}}</view></view></view></view><view class="text-content text-cut">{{item.content}}</view><view class="{{['grid flex-sub padding-lr',isCard?'col-3 grid-square':'col-1']}}"><view class="grid flex-sub padding-lr col-3 grid-square"><block wx:for="{{item.images}}" wx:for-item="image" wx:for-index="index" wx:key="index"><view class="bg-img" style="{{'background-image:'+('url('+image.imgpath+')')+';'}}"></view></block></view></view><view class="padding-sm radius margin-top-sm text-sm bg-white"><view><block wx:for="{{item.comments}}" wx:for-item="comment" wx:for-index="index" wx:key="index"><view class="flex"><view>{{comment.commentUser.nickname+"："}}</view><view class="text-df margin-right text-cut" style="width:75%;">{{comment.content}}</view></view></block></view><view class="text-gray text-sm text-right padding"><text data-event-opts="{{[['tap',[['toggleLike',['$0'],[[['findListres.list','',index]]]]]]]}}" class="cuIcon-appreciatefill margin-lr-xs" style="{{'color:'+(item.likePraise==true?'red':'')+';'}}" catchtap="__e"></text>{{''+item.likeNumber+''}}<text class="cuIcon-messagefill margin-lr-xs"></text>{{''+item.numberComments+''}}</view></view></view></view></block><block wx:if="{{currPage===totalPage||totalPage==''}}"><view><tui-divider vue-id="2" width="80%" bind:__l="__l" vue-slots="{{['default']}}">没有更多啦</tui-divider></view></block></view>