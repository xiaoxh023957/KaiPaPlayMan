<view><cu-custom class="text-white" vue-id="1" bgColor="kpbg-black" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view class="text-white" slot="backText">返回</view><view class="text-white" slot="content">需求提交</view></cu-custom><form><view class="cu-form-group margin-top"><view class="title" style="width:180rpx;height:100%;text-align-last:justify;">姓名：</view><input name="input" data-event-opts="{{[['input',[['__set_model',['','nickname','$event',[]]]]]]}}" value="{{nickname}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">需求标题：</view><input name="input" data-event-opts="{{[['input',[['__set_model',['','title','$event',[]]]]]]}}" value="{{title}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机号码：</view><input type="number" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/><view class="cu-capsule radius"><view class="cu-tag bg-blue ">+86</view><view class="cu-tag line-blue">中国大陆</view></view></view><view class="cu-form-group"><view class="title">店铺类型：</view><picker value="{{index}}" range="{{picker}}" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(index>-1?picker[index]:info)+''}}</view></picker></view><view class="margin-tb  padding-bottom"><text class="margin-left text-grey">需求内容：</text><view class="cu-form-group margin-tb-xs "><textarea maxlength="-1" disabled="{{modalName!=null}}" placeholder="您可以在这里把您的需求提交给我们，让我们为您定制满意的服务。" data-event-opts="{{[['input',[['__set_model',['','content','$event',[]]],['textareaAInput',['$event']]]]]}}" value="{{content}}" bindinput="__e"></textarea></view></view><view class="padding flex flex-direction " style="width:400rpx;margin:auto;"><button data-event-opts="{{[['tap',[['save']]]]}}" class="cu-btn bg-green lg text-center" bindtap="__e">提交</button></view></form></view>